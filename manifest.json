{
  "name": "Wort of the tab",
  "description": "Every tab a new german word!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  },
  "permissions": ["storage", "scripting", "tabs"],
  "action": {
    "default_icon": {
      "16": "/images/cavalo16.png",
      "32": "/images/cavalo32.png",
      "48": "/images/cavalo48.png",
      "128": "/images/cavalo128.png"
    }
  },
  "icons": {
    "16": "/images/cavalo16.png",
    "32": "/images/cavalo32.png",
    "48": "/images/cavalo48.png",
    "128": "/images/cavalo128.png"
  }
}
